  "author": "Radio Net Media Limited / AdTonos / Wojtek Lichota",
  "license": "MIT",
  "homepage": "https://github.com/adtonos/jovo-plugin-alexa-skill-connections#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/adtonos/jovo-plugin-alexa-skill-connections"
  },
  "bugs": {
    "url": "https://github.com/adtonos/jovo-plugin-alexa-skill-connections/issues"
  },
  "dependencies": {
    "jovo-core": "^2.2.10",
    "jovo-platform-alexa": "^2.2.28"
  },
  "devDependencies": {
