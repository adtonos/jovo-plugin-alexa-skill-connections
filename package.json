    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "jest": "^24.8.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.8",
    "ts-jest": "^24.0.1",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.1"
  },
  "jest": {
    "transform": {
